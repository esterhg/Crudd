<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="3dp">

    <EditText
        android:id="@+id/txtgenero"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="82dp"
        android:layout_marginBottom="10dp"
        android:ems="10"
        android:inputType="text"
        android:text=""
        android:textSize="15dp"
        app:layout_constraintBottom_toTopOf="@+id/txtfecha"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/Url" />

    <TextView
        android:id="@+id/txtimg"
        android:layout_width="31dp"
        android:layout_height="30dp"
        android:layout_marginStart="59dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="54dp"
        android:text="Url"
        android:textSize="15dp"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintEnd_toStartOf="@+id/Url"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/txtnombr"
        android:layout_width="65dp"
        android:layout_height="18dp"
        android:layout_marginStart="42dp"
        android:layout_marginTop="132dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="76dp"
        android:text="Nombre"
        android:textSize="15dp"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintEnd_toStartOf="@+id/txtnom"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/Actua"
        android:layout_width="89dp"
        android:layout_height="31dp"
        android:layout_marginTop="46dp"
        android:layout_marginBottom="47dp"
        android:text="Actualizar"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/txtnom"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/Url"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="72dp"
        android:layout_marginBottom="29dp"
        android:inputType="text"
        android:text=""
        android:textSize="15dp"
        app:layout_constraintBottom_toTopOf="@+id/txtgenero"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/txtimg"
        app:layout_constraintTop_toBottomOf="@+id/txtapellido"
        tools:ignore="DuplicateIds" />

    <EditText
        android:id="@+id/txtnom"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="72dp"
        android:layout_marginBottom="44dp"
        android:inputType="text"
        android:text=""
        android:textSize="15dp"
        app:layout_constraintBottom_toTopOf="@+id/txtapellido"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/txtnombr"
        app:layout_constraintTop_toBottomOf="@+id/Actua"
        tools:ignore="DuplicateIds" />

    <EditText
        android:id="@+id/txtapellido"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="67dp"
        android:layout_marginBottom="23dp"
        android:ems="10"
        android:inputType="text"
        android:text=""
        android:textSize="15dp"
        app:layout_constraintBottom_toTopOf="@+id/Url"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/txtnom" />

    <EditText
        android:id="@+id/txtfecha"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="52dp"
        android:layout_marginBottom="66dp"
        android:ems="10"
        android:inputType="text"
        android:text=""
        android:textSize="15dp"
        app:layout_constraintBottom_toTopOf="@+id/btnActualizar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/txtgenero" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="42dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="41dp"
        android:text="Apellidos"
        app:layout_constraintBottom_toTopOf="@+id/txtimg"
        app:layout_constraintEnd_toStartOf="@+id/txtapellido"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtnombr" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="47dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="58dp"
        android:text="Genero"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintEnd_toStartOf="@+id/txtgenero"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtimg" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="47dp"
        android:layout_marginEnd="9dp"
        android:layout_marginBottom="267dp"
        android:text="Fecha Nacimiento"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/txtfecha"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <Button
        android:id="@+id/btnActualizar"
        android:layout_width="133dp"
        android:layout_height="0dp"
        android:layout_marginBottom="144dp"
        android:backgroundTint="#0074BD"
        android:text="Actualizar"
        android:textColor="@android:color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtfecha" />


</androidx.constraintlayout.widget.ConstraintLayout>